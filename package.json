{
  "name": "pa11y-sample-project",
  "version": "1.0.0",
  "description": "Example project demonstrating accessibility testing using pa11y",
  "main": "index.js",
  "scripts": {
    "pa11y": "node pa11y/index.js",
    "pa11y-ci": "pa11y-ci --config pa11y-ci/config.pa11yci -j > pa11y-ci/reports/pa11y-ci-results.json",
    "pa11y-ci-report": "pa11y-ci-reporter-html -s pa11y-ci/reports/pa11y-ci-results.json -d  pa11y-ci/reports --include-zero-issues"
  },
  "author": "sansong",
  "devDependencies": {},
  "dependencies": {
    "pa11y": "^5.3.0",
    "pa11y-reporter-html": "^1.0.0",
    "pa11y-ci": "^2.3.0",
    "pa11y-ci-reporter-html": "^2.0.2"
  }
}
